{
  "name": "antd-country-phone-input",
  "version": "3.0.1",
  "description": "Country phone input component as standard Ant.Design form item",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "build:es": "rimraf es && tsc --module es6 --outDir es --noEmit false",
    "build:lib": "rimraf lib && tsc --module commonjs --outDir lib --noEmit false",
    "prepare": "npm-run-all --parallel build:*"
  },
  "repository": "git@github.com:boyuai/antd-country-phone-input.git",
  "keywords": [
    "antd",
    "ant-design",
    "country-code",
    "phone"
  ],
  "author": "wangsijie <i@sijie.wang>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "antd": "^4.5.2",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "peerDependencies": {
    "antd": "^4.5.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
